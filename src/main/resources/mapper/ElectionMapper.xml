<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.mason.practice.dao.ElectionBasicInfoDao">
    <select id="queryElectionInfo" parameterType="String" resultType="com.mason.practice.pojo.Election">
        select stockabbr as stockAbbr,
               stockcode as stockCode
        from election_info limit 20
    </select>

</mapper>